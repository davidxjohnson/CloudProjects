{
  "name": "cloud-projects",
  "version": "1.0.0",
  "description": "TypeScript monorepo containing AWS CDK projects and AWS SDK examples",
  "main": "index.js",
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "lint": "./lint.sh",
    "clean": "pnpm -r exec rm -rf dist lib node_modules",
    "install:all": "pnpm install"
  },
  "keywords": [
    "aws",
    "cdk",
    "typescript",
    "kubernetes",
    "infrastructure",
    "examples"
  ],
  "author": "David Johnson",
  "license": "ISC",
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": ">=18.19.1",
    "npm": ">=8.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "@aws-cdk/lambda-layer-kubectl-v33": "2.0.0",
    "aws-cdk-lib": "2.217.0",
    "commander": "13.1.0",
    "constructs": "10.4.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/node": "24.6.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.12.0",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.45.0"
  },
  "pnpm": {
    "overrides": {
      "esbuild": "0.25.0"
    }
  }
}