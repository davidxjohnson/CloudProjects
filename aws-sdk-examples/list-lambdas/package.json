{
  "name": "@cloud-projects/list-lambdas-example",
  "version": "1.0.0",
  "description": "TypeScript example for listing AWS Lambda functions",
  "type": "module",
  "main": "dist/list-lambdas.js",
  "engines": {
    "node": ">=18.19.1"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc --watch",
    "start": "node dist/list-lambdas.js",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "3.896.0",
    "@aws-sdk/types": "3.893.0",
    "commander": "12.1.0"
  },
  "devDependencies": {
    "@types/node": "22.18.6",
    "@vitest/coverage-v8": "2.1.9",
    "typescript": "5.9.2",
    "vitest": "2.1.9"
  }
}